# Build rows for table
rows = []
for name, details in matched_uat_prod_records.items():
    rows.append({
        "Dataset": name,
        "PROD Count": details["PROD"]["count"],
        "UAT Count": details["UAT"]["count"],
        "Matched": details["matching"]["matched_count"],
        "Unmatched": details["matching"]["unmatched_count"],
        "Status (PROD)": details["PROD"]["status"],
        "Status (UAT)": details["UAT"]["status"]
    })

